def calculateArcLength(arc_len, arc_height):
    radiusone = arc_height / 2
    radiustwo = (arc_len * arc_len) / (8* arc_height)
    total_radius = radiusone + radiustwo
    print(total_radius)
    theta = 2 * sin^-1 (arc_len/total_radius * 2)
    print(theta)
